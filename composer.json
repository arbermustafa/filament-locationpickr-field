{
    "name": "arbermustafa/filament-locationpickr-field",
    "description": "Location picker field for Filament Php using Google Maps",
    "keywords": [
        "arbermustafa",
        "laravel",
        "filament-plugin",
        "filament-locationpickr-field",
        "google-maps",
        "maps",
        "form",
        "field",
        "php"
    ],
    "homepage": "https://github.com/arbermustafa/filament-locationpickr-field",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arber Mustafa",
            "email": "info@arbermustafa.com",
            "role": "Developer",
            "homepage": "https://arbermustafa.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^2.0",
        "spatie/laravel-package-tools": "^1.13.5",
        "illuminate/contracts": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-livewire": "^1.0",
        "pestphp/pest-plugin-parallel": "^0.3",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ArberMustafa\\FilamentLocationPickrField\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArberMustafa\\FilamentLocationPickrField\\Tests\\": "tests"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "test:pest": "vendor/bin/pest --parallel",
        "test-coverage": "vendor/bin/pest --coverage",
        "test": [
            "@test:pest"
        ],
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArberMustafa\\FilamentLocationPickrField\\LocationPickrFieldServiceProvider"
            ],
            "aliases": {
                "LocationPickrField": "ArberMustafa\\FilamentLocationPickrField\\LocationPickrField"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
