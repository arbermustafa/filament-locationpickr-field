{
    "private": true,
    "type": "module",
    "scripts": {
        "dev:styles": "npx tailwindcss -i resources/css/index.css -o resources/dist/filament-locationpickr-field.css --postcss --watch",
        "dev:scripts": "node bin/build.js --dev",
        "build:styles": "npx tailwindcss -i resources/css/index.css -o resources/dist/filament-locationpickr-field.css --postcss --minify && npm run purge",
        "build:scripts": "node bin/build.js",
        "prettier": "npx prettier --write resources/{**/*,*}.{css,js,jsx,blade.php}",
        "purge": "filament-purge -i resources/dist/filament-locationpickr-field.css -o resources/dist/filament-locationpickr-field.css -v 3.x",
        "dev": "npm-run-all --parallel dev:*",
        "build": "npm run prettier && npm-run-all build:*"
    },
    "devDependencies": {
        "@awcodes/filament-plugin-purge": "^1.1.1",
        "@shufo/prettier-plugin-blade": "^1.11.1",
        "autoprefixer": "^10.4.14",
        "esbuild": "^0.19.2",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.26",
        "postcss-import": "^15.1.0",
        "prettier": "^3.0.0",
        "prettier-plugin-tailwindcss": "^0.4.1",
        "tailwindcss": "^3.3.3"
    },
    "dependencies": {
        "@googlemaps/js-api-loader": "^1.16.2"
    }
}
